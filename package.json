{
  "name": "backend",
  "version": "1.6.1",
  "description": "Proyecto Base para el inicio de proyectos basados en NestJS",
  "author": "mlaura",
  "private": true,
  "license": "LPG-Bolivia",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "setup": "npm run migrations:clean; npm run schema:drop; npm run migrations:generate database/migrations/init; npm run migrations:run; npm run seeds:run;",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:devsql": "NODE_ENV=development FORCE_SQL_LOG=true nest start --watch",
    "start:database": "./scripts/database.sh",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --verbose --coverage=false",
    "test:single": "NODE_ENV=test jest --coverage=false -i",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:cov": "NODE_ENV=test jest --coverage",
    "test:debug": "NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migrations:clean": "rimraf database/migrations/*;",
    "migrations:generate": "npm run typeorm -- migration:generate -d ormconfig-default.ts",
    "migrations:run": "npm run typeorm -- migration:run -d ormconfig-default.ts",
    "migrations:revert": "npm run typeorm -- migration:revert -d ormconfig-default.ts",
    "seeds:create": "npm run typeorm -- migration:create",
    "seeds:run": "npm run typeorm -- migration:run -d ormconfig-seed.ts",
    "schema:drop": "npm run typeorm -- schema:drop -d ormconfig-default.ts",
    "rimraf": "./node_modules/rimraf/bin.js",
    "compodoc": "compodoc -p tsconfig.json -o --language es-ES --disableCoverage --customLogo 'docs/imagenes/logo.png' --hideGenerator",
    "db:diagram": "ts-node ./node_modules/.bin/typeorm-uml ormconfig-default.ts -d ormconfig-default.ts -d docs/ERD.png",
    "prepare": "husky install",
    "check-node-version": "node ./node_modules/.bin/check-node-version",
    "prebuild": "npm run check-node-version && rimraf dist",
    "presetup": "npm run check-node-version",
    "prestart": "npm run check-node-version",
    "prestart:dev": "npm run check-node-version",
    "pretest": "npm run check-node-version",
    "preinstall": "mkdir -p node_modules; touch ./node_modules/.metadata_never_index",
    "postinstall": "npm run check-node-version",
    "release": "standard-version"
  },
  "dependencies": {
    "@elartedelcodigo/check-node-version": "1.1.4",
    "@golevelup/ts-jest": "0.3.3",
    "@nestjs/axios": "0.1.0",
    "@nestjs/common": "9.1.2",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.1.2",
    "@nestjs/jwt": "9.0.0",
    "@nestjs/passport": "9.0.0",
    "@nestjs/platform-express": "9.1.2",
    "@nestjs/schedule": "2.1.0",
    "@nestjs/swagger": "6.1.2",
    "@nestjs/typeorm": "9.0.1",
    "@types/bcrypt": "5.0.0",
    "@types/express-list-endpoints": "6.0.0",
    "bcrypt": "5.0.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "cls-rtracer": "2.6.2",
    "connect-typeorm": "2.0.0",
    "cookie-parser": "1.4.6",
    "csurf": "1.11.0",
    "dayjs": "1.11.5",
    "dotenv": "16.0.3",
    "ejs": "3.1.8",
    "express-list-endpoints": "6.0.0",
    "express-session": "1.17.3",
    "fast-redact": "3.1.2",
    "helmet": "6.0.0",
    "html-to-pdfmake": "2.4.6",
    "husky": "8.0.1",
    "ip": "1.1.8",
    "jsdom": "20.0.0",
    "nanoid": "3.3.4",
    "nest-authz": "2.2.1",
    "nestjs-pino": "3.1.1",
    "openid-client": "5.1.10",
    "passport": "0.6.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "pdfmake": "0.2.5",
    "pg": "8.8.0",
    "pino": "8.6.1",
    "pino-elasticsearch": "6.2.0",
    "pino-http": "8.2.0",
    "pino-http-send": "0.4.2",
    "pino-pretty": "9.1.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rotating-file-stream": "3.0.4",
    "rxjs": "7.5.7",
    "swagger-ui-express": "4.5.0",
    "typeorm": "0.3.10",
    "typeorm-adapter": "1.5.2",
    "uuid": "9.0.0",
    "zxcvbn-typescript": "5.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@compodoc/compodoc": "1.1.19",
    "@nestjs/cli": "9.1.4",
    "@nestjs/schematics": "9.0.3",
    "@nestjs/testing": "9.1.2",
    "@types/cookie-parser": "1.4.3",
    "@types/cron": "2.0.0",
    "@types/express": "4.17.14",
    "@types/express-session": "1.17.5",
    "@types/fast-redact": "3.0.2",
    "@types/ip": "1.1.0",
    "@types/jest": "29.1.1",
    "@types/node": "18.7.23",
    "@types/passport-local": "1.0.34",
    "@types/pdfmake": "0.2.1",
    "@types/supertest": "2.0.12",
    "@types/uuid": "8.3.4",
    "@types/zxcvbn": "4.4.1",
    "@typescript-eslint/eslint-plugin": "5.38.1",
    "@typescript-eslint/parser": "5.38.1",
    "eslint": "8.24.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.1.2",
    "jest-sonar-reporter": "2.0.0",
    "prettier": "2.7.1",
    "standard-version": "9.5.0",
    "supertest": "6.2.4",
    "ts-jest": "29.0.3",
    "ts-loader": "9.4.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "src/main.ts",
      "src/database/migrations/",
      "src/database/seeds/",
      ".module.ts",
      ".entity.ts",
      ".dto.ts",
      ".guard.ts",
      ".enum.ts"
    ],
    "collectCoverage": true,
    "testResultsProcessor": "jest-sonar-reporter"
  },
  "engines": {
    "node": "^16",
    "npm": "^8"
  }
}
